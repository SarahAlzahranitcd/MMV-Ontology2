    <!-- http://w3id.org/mmvhasDomainAssumption -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmvhasDomainAssumption">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:comment>Captures assumptions about domain semantics or modelling expectations that affect mapping interpretation, helping others understand potential validity limits during reuse.</rdfs:comment>
        <rdfs:label>hasDomainAssumption</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmvhasInputDescription -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmvhasInputDescription">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:comment>Provides a concise description of the input dataset content and scope, to support understanding and reuse without requiring full dataset publication.</rdfs:comment>
        <rdfs:label>hasInputDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmvhasInputName -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmvhasInputName">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:comment>Captures the human-readable name of an input dataset (e.g., a file name), supporting traceability of the source used during mapping creation.</rdfs:comment>
        <rdfs:label>hasInputName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmvhasMappingDomain -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmvhasMappingDomain">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:comment>Indicates the subject area or domain the mapping addresses (e.g., cultural heritage, geospatial), supporting discovery and contextual reuse.</rdfs:comment>
        <rdfs:label>hasMappingDomain</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmvhasRole -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmvhasRole">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Records the stakeholder role in the mapping project (e.g., developer, reviewer, domain expert), supporting accountability and provenance-oriented interpretation.</rdfs:comment>
        <rdfs:label>hasRole</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmvhasTechnicalRequirement -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmvhasTechnicalRequirement">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:comment>Records technical constraints that influence implementation choices (e.g., required tool support or execution environment), supporting reproducibility and future maintenance decisions.</rdfs:comment>
        <rdfs:label>hasTechnicalRequirement</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasAssumption -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasAssumption">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Records assumptions made during analysis about the source data, target model, or transformation logic, supporting correct interpretation and reuse.</rdfs:comment>
        <rdfs:label>hasAssumption</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasBackground -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasBackground">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Summarises relevant stakeholder expertise or experience, providing interpretive context for design and validation decisions.</rdfs:comment>
        <rdfs:label>hasBackground</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasDescription -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasDescription">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#MappingArtifact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Provides a concise description of a mapping artefact or project element to support understanding and discovery.</rdfs:comment>
        <rdfs:label>hasDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasDesignDecision -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasDesignDecision">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#DesignActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Captures final design choices made during the design phase (e.g., modelling patterns, reuse choices), enabling rationale-aware reuse and comparison.</rdfs:comment>
        <rdfs:label>hasDesignDecision</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasJustification -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasJustification">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#DesignActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Provides the rationale for a recorded design decision, supporting transparency and helping future maintainers assess trade-offs.</rdfs:comment>
        <rdfs:label>hasJustification</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasPublisherName -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasPublisherName">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#MaintenanceActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Identifies the person or organisation responsible for publishing or releasing the mapping artefact, supporting provenance and citation.</rdfs:comment>
        <rdfs:label>hasPublisherName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasPurpose -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasPurpose">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Describes the intended purpose or goal of the mapping project.</rdfs:comment>
        <rdfs:label>hasPurpose</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasRequirement -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasRequirement">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>requirements of the project</rdfs:comment>
        <rdfs:label>hasRequirement</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasRiskOrIssue -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasRiskOrIssue">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#AnalysisActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Risk or issues realted to the project</rdfs:comment>
        <rdfs:label>hasRiskOrIssue</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasTestingResult -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasTestingResult">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#TestingActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Records the outcome of a testing activity (e.g., pass/fail summary), supporting quality assessment and reuse decisions.</rdfs:comment>
        <rdfs:label>hasTestingResult</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#hasTestingType -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#hasTestingType">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#QualityAssessment"/>
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#TestingActivity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the type of testing or validation performed (e.g., SHACL validation, unit testing), supporting assessment of mapping trustworthiness.</rdfs:comment>
        <rdfs:label>hasTestingType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://w3id.org/mmv/#version -->


    <owl:DatatypeProperty rdf:about="http://w3id.org/mmv/#version">
        <rdfs:domain rdf:resource="http://w3id.org/mmv/#MappingArtifact"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Records a version identifier for the mapping artefact, enabling change tracking and controlled reuse across releases.</rdfs:comment>
        <rdfs:label>version</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://alex-randles.github.io/MQV/#hasMappingFormat -->


    <owl:DatatypeProperty rdf:about="https://alex-randles.github.io/MQV/#hasMappingFormat">
        <rdfs:comment>Reuse of the MQV property to specify the concrete syntax or encoding format in which a mapping definition is expressed, such as RML, R2RML, SPARQL, or SSSOM.</rdfs:comment>
        <rdfs:isDefinedBy>https://alex-randles.github.io/MQV/</rdfs:isDefinedBy>
        <rdfs:label>hasMappingFormat</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://alex-randles.github.io/MQV/#hasMappingMethod -->


    <owl:DatatypeProperty rdf:about="https://alex-randles.github.io/MQV/#hasMappingMethod">
        <rdfs:comment>Reuse of the MQV property to describe the method by which a mapping was produced, such as manual, semi-automatic, or fully automatic generation.</rdfs:comment>
        <rdfs:isDefinedBy>https://alex-randles.github.io/MQV/</rdfs:isDefinedBy>
        <rdfs:label>hasMappingMethod</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://alex-randles.github.io/MQV/#hasMappingType -->


    <owl:DatatypeProperty rdf:about="https://alex-randles.github.io/MQV/#hasMappingType">
        <rdfs:comment>Reuse of the MQV property to indicate the type of mapping being created, for example uplift mapping, ontology alignment, or interlinking.</rdfs:comment>
        <rdfs:isDefinedBy>https://alex-randles.github.io/MQV/</rdfs:isDefinedBy>
        <rdfs:label>hasMappingType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
